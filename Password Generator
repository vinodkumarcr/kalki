import random
no=int(input('How many passwords do you want: '))
length=int(input('Provide the password length: '))
data='abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890!@#$%^&*()_-+'
password=[]
for a in range(no):
    for b in range(length):
        datas=random.choice(data)
        password.append(datas)
        passwords=''.join(password)
    print(passwords)
    password.clear()
        
